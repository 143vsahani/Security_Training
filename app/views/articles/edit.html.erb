<h1>Edit User</h1>

<%= form_with(model: @article, local: true) do |form| %>
  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="field">
    <%= form.label :body %>
    <%= form.text_field :body %>
  </div>

  <div class="actions">
    <%= form.submit 'Update User' %>
  </div>
<% end %>

<%= link_to 'Back', article_path %>
